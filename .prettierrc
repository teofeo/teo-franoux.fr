{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 150,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.html", "*.md"],
      "options": {
        "printWidth": 120
      }
    }
  ]
}

